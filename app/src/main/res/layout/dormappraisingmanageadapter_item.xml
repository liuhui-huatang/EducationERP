<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:background="@mipmap/ssgl_pycx_bg"
        android:layout_marginLeft="@dimen/common_10"
        android:layout_marginRight="@dimen/common_10"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="@dimen/common_30"
        android:layout_marginRight="@dimen/common_20"
        android:layout_centerVertical="true"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/common_10"
                android:text="宿舍号"
                android:textColor="@color/ssgl_tv13_color"
                android:textSize="@dimen/textSize_14"
                android:layout_marginLeft="@dimen/common_10"
                />
            <TextView
                android:id="@+id/dormappraisingmanageadapter_ss_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/common_10"
                android:paddingRight="@dimen/common_10"
                android:paddingBottom="@dimen/common_10"
                android:text="1号楼202"
                android:layout_alignParentRight="true"
                android:textColor="@color/ssgl_tv14_color"
                android:layout_marginLeft="@dimen/common_10"
                android:textSize="@dimen/textSize_14"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/common_10"
                android:text="奖项"
                android:textColor="@color/ssgl_tv13_color"
                android:textSize="@dimen/textSize_14"
                />
            <TextView
                android:id="@+id/dormappraisingmanageadapter_jx_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/common_10"
                android:paddingRight="@dimen/common_10"
                android:paddingBottom="@dimen/common_10"
                android:text="一等奖"
                android:layout_alignParentRight="true"
                android:textColor="@color/ssgl_tv14_color"
                android:textSize="@dimen/textSize_14"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/common_10"
                android:text="区域"
                android:textColor="@color/ssgl_tv13_color"
                android:textSize="@dimen/textSize_14"
                />
            <TextView
                android:id="@+id/dormappraisingmanageadapter_qy_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/common_10"
                android:paddingRight="@dimen/common_10"
                android:paddingBottom="@dimen/common_10"
                android:text="宿舍地面"
                android:layout_alignParentRight="true"
                android:textColor="@color/ssgl_tv14_color"
                android:textSize="@dimen/textSize_14"
                />
        </LinearLayout>
    </LinearLayout>
        <TextView
            android:id="@+id/dormappraisingmanageadapter_date_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_5"
            android:layout_marginRight="@dimen/common_10"
            android:layout_marginTop="@dimen/common_5"
            android:text="2016-10-23"
            android:textColor="@color/ssgl_tv3_color"
            android:textSize="@dimen/textSize_12"
            android:layout_alignParentRight="true"
            />
        <RelativeLayout
            android:layout_width="@dimen/common_40"
            android:layout_height="@dimen/common_40"
            android:background="@mipmap/ssgl_pyss_icon1"
            android:layout_centerVertical="true"
            >
            <TextView
                android:id="@+id/dormappraisingmanageadapter_count_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="96"
                android:textColor="@color/white"
                android:textSize="@dimen/textSize_12"
                android:layout_centerInParent="true"
                />
            </RelativeLayout>
    </RelativeLayout>
</LinearLayout>