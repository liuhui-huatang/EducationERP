            android:textColor="@color/jwgl_tv3_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/login_line_bg"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="总成绩"
            android:textColor="@color/jwgl_tv2_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            />
        <TextView
            android:id="@+id/jwbjcjxq_zcj_tv"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="32"
            android:textColor="@color/jwgl_tv3_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/common_10"
        >
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:text="课程"
            android:textColor="@color/jwgl_tv5_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            android:gravity="center"
            />
        <TextView
            android:layout_width="0dp"
            android:layout_weight="1.0"
            android:layout_height="wrap_content"
            android:text="分数"
            android:textColor="@color/jwgl_tv5_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            android:gravity="center"
            />
    </LinearLayout>
    <com.htcompany.educationerpforgansu.commonpart.views.GDListView
        android:id="@+id/eductionexamxq_lv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
</LinearLayout>