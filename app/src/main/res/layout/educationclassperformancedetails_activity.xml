            android:layout_height="wrap_content"
            android:text="教学班"
            android:textColor="@color/jwgl_tv2_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            />
        <TextView
            android:id="@+id/jwbjcjxq_jxb_tv"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="李看天"
            android:textColor="@color/jwgl_tv3_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
       android:background="@color/login_line_bg"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="姓名"
            android:textColor="@color/jwgl_tv2_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            />
        <TextView
            android:id="@+id/jwbjcjxq_xm_tv"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="李看天"
            android:textColor="@color/jwgl_tv3_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
            />
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/login_line_bg"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="学号"
            android:textColor="@color/jwgl_tv2_color"
            android:padding="@dimen/common_10"
            android:textSize="@dimen/textSize_14"
