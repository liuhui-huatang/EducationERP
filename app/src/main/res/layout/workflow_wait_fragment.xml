<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.htcompany.educationerpforgansu.commonpart.views.xListView.XListView
        android:id="@+id/workflow_dbsx_lv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:dividerHeight="@dimen/common_3"
        android:divider="@color/activity_bg_color"
        />
    <ImageView
        android:id="@+id/workflow_dbsx_wsj_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/wushuju_bg"
        android:scaleType="fitXY"
        android:visibility="gone"
        />
    <LinearLayout
        android:id="@+id/workflow_shaixuan_ll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@mipmap/wflow_shaixuan_icon"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="50dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:gravity="center"
        android:visibility="gone"
        >
    </LinearLayout>

</RelativeLayout>