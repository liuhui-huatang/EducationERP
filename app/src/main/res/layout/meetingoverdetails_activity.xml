<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:background="@color/activity_bg_color"
    >
    <include
        layout="@layout/common_title_bar"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <LinearLayout
            android:id="@+id/mwumeeting_bottompart_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="@color/white"
            android:layout_alignParentBottom="true"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="删除"
                android:background="@drawable/login_shape_btn"
                android:textColor="@color/title_bar_color"
                android:padding="@dimen/common_10"
                android:layout_margin="@dimen/common_10"
                android:gravity="center"
                />
        </LinearLayout>
        <ScrollView
            android:layout_above="@id/mwumeeting_bottompart_ll"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                >
                <include
                    layout="@layout/meetingdetails_contentpart"
                    />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:layout_marginTop="@dimen/common_10"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/textSize_14"
                        android:textColor="@color/black2"
                        android:text="审核人"
                        android:padding="@dimen/common_10"
                        />
                    <TextView
                        android:id="@+id/meetingoverdetails_shr_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/textSize_14"
                        android:textColor="@color/black2"
                        android:layout_alignParentRight="true"
                        android:text="二狗蛋"
                        android:padding="@dimen/common_10"
                        />
                </RelativeLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/login_line_bg"
                    />
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/textSize_14"
                        android:textColor="@color/black2"
                        android:text="审核时间"
                        android:padding="@dimen/common_10"
                        />
                    <TextView
                        android:id="@+id/meetingoverdetails_shtime_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/textSize_14"
                        android:textColor="@color/black2"
                        android:layout_alignParentRight="true"
                        android:text="2016-11-23 15:23"
                        android:padding="@dimen/common_10"
                        />
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
</LinearLayout>