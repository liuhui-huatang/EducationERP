<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:orientation="horizontal"
        android:background="@color/white"
        >
        <LinearLayout
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center"
            android:layout_marginLeft="@dimen/common_20"
            android:layout_gravity="center_vertical"
            >
            <com.htcompany.educationerpforgansu.commonpart.views.XCRoundAndOvalImageView
                android:id="@+id/personsocialinsuranceadapter_photo_img"
                android:layout_width="@dimen/common_40"
                android:layout_height="@dimen/common_40"
                android:src="@mipmap/me_hredcokn"
                android:scaleType="fitXY"
                />
            <TextView
                android:id="@+id/personsocialinsuranceadapter_name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/textSize_14"
                android:textColor="@color/ssgl_tv11_color"
                android:text="李红"
                android:padding="@dimen/common_5"
                android:textStyle="bold"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center_vertical"
            android:layout_marginLeft="@dimen/common_30"
            android:layout_gravity="center_vertical"
            >
                <TextView
                    android:id="@+id/personsocialinsuranceadapter_starttime_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/textSize_14"
                    android:textColor="@color/ssgl_tv11_color"
                    android:text="缴纳开始时间：教师"
                    android:padding="@dimen/common_10"
                    />

            <TextView
                android:id="@+id/personsocialinsuranceadapter_code_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/common_10"
                android:text="社会保险号码：20171245"
                android:textColor="@color/ssgl_tv11_color"
                android:textSize="@dimen/textSize_14" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>