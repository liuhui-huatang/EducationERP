                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="水电管理"
                    android:textColor="@color/black2"
                    android:textSize="@dimen/textSize_12"
                    android:gravity="center"
                    android:padding="@dimen/common_10"
