<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:background="@color/activity_bg_color"
    >
    <include
        layout="@layout/common_title_bar"
        />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/common_5"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:text="会议名称"
            android:textColor="@color/black2"
            android:textSize="@dimen/textSize_14"
            />
        <EditText
            android:id="@+id/meetingapply_name_edt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:layout_alignParentRight="true"
            android:hint="输入会议名称"
            android:textColorHint="@color/login_line_bg"
            android:textColor="@color/title_bar_color"
            android:textSize="@dimen/textSize_14"
            android:background="#00000000"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/login_line_bg"
        android:layout_marginLeft="@dimen/common_10"
        android:layout_marginRight="@dimen/common_10"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/white"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:text="会议编号"
            android:textColor="@color/black2"
            android:textSize="@dimen/textSize_14"
            />
        <EditText
            android:id="@+id/meetingapply_code_edt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:layout_alignParentRight="true"
            android:hint="输入会议编号"
            android:textColorHint="@color/login_line_bg"
            android:textColor="@color/title_bar_color"
            android:textSize="@dimen/textSize_14"
            android:background="#00000000"
            />
    </LinearLayout>
    <RelativeLayout
        android:id="@+id/meetingapply_starttime_rel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:layout_marginTop="@dimen/common_5"
        >
        <ImageView
            android:id="@+id/mymeetingapply_start_img"
            android:layout_width="@dimen/common_10"
            android:layout_height="@dimen/common_10"
            android:scaleType="fitXY"
            android:src="@mipmap/qj_icon_ty_green"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/common_10"
            />
        <TextView
            android:layout_toRightOf="@id/mymeetingapply_start_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:text="开始时间"
            android:textColor="@color/black2"
            android:textSize="@dimen/textSize_14"
            />
        <TextView
            android:id="@+id/meetingapply_starttime_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:layout_alignParentRight="true"
            android:hint="选择会议开始时间"
            android:drawableRight="@mipmap/me_icon5"
            android:drawablePadding="@dimen/common_10"
            android:textColorHint="@color/login_line_bg"
            android:gravity="center_vertical|right"
            android:textColor="@color/title_bar_color"
            android:textSize="@dimen/textSize_14"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/login_line_bg"
        android:layout_marginLeft="@dimen/common_10"
        android:layout_marginRight="@dimen/common_10"
        />
    <RelativeLayout
        android:id="@+id/meetingapply_endtime_rel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >
        <ImageView
            android:id="@+id/mymeetingapply_end_img"
            android:layout_width="@dimen/common_10"
            android:layout_height="@dimen/common_10"
            android:scaleType="fitXY"
            android:src="@mipmap/qj_icon_ty_red"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="@dimen/common_10"
            />
        <TextView
            android:layout_toRightOf="@id/mymeetingapply_end_img"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:text="结束时间"
            android:textColor="@color/black2"
            android:textSize="@dimen/textSize_14"
            />
        <TextView
            android:id="@+id/meetingapply_endtime_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:layout_alignParentRight="true"
            android:hint="选择会议结束时间"
            android:drawableRight="@mipmap/me_icon5"
            android:drawablePadding="@dimen/common_10"
            android:textColorHint="@color/login_line_bg"
            android:gravity="center_vertical|right"
            android:textColor="@color/title_bar_color"
            android:textSize="@dimen/textSize_14"
            />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/meetingapply_adres_rel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:layout_marginTop="@dimen/common_5"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:text="召开地点"
            android:textColor="@color/black2"
            android:textSize="@dimen/textSize_14"
            />
        <TextView
            android:id="@+id/meetingapply_adres_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:layout_alignParentRight="true"
            android:hint="选择会议召开地点"
            android:drawableRight="@mipmap/me_icon5"
            android:drawablePadding="@dimen/common_10"
            android:textColorHint="@color/login_line_bg"
            android:gravity="center_vertical|right"
            android:textColor="@color/title_bar_color"
            android:textSize="@dimen/textSize_14"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/login_line_bg"
        android:layout_marginLeft="@dimen/common_10"
        android:layout_marginRight="@dimen/common_10"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:text="主持人"
            android:textColor="@color/black2"
            android:textSize="@dimen/textSize_14"
            />
        <TextView
            android:id="@+id/meetingapply_zcr_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:layout_alignParentRight="true"
            android:hint="选择会议主持人"
            android:drawableRight="@mipmap/me_icon5"
            android:drawablePadding="@dimen/common_10"
            android:textColorHint="@color/login_line_bg"
            android:gravity="center_vertical|right"
            android:textColor="@color/title_bar_color"
            android:textSize="@dimen/textSize_14"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/login_line_bg"
        android:layout_marginLeft="@dimen/common_10"
        android:layout_marginRight="@dimen/common_10"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        >
        <RelativeLayout
            android:id="@+id/meetingapply_chry_rel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/common_10"
                android:text="参会人员"
                android:textColor="@color/black2"
                android:textSize="@dimen/textSize_14"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="@dimen/common_10"
                android:layout_alignParentRight="true"
                android:hint="选择参会人员"
                android:drawableRight="@mipmap/me_icon5"
                android:drawablePadding="@dimen/common_10"
                android:textColorHint="@color/login_line_bg"
                android:gravity="center_vertical"
                android:textColor="@color/title_bar_color"
                android:textSize="@dimen/textSize_14"
                />
        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:layout_marginLeft="@dimen/common_20"
            android:layout_marginRight="@dimen/common_20"
            android:layout_marginBottom="@dimen/common_10"
            android:background="@drawable/login_shape_editxt"
            >
            <com.htcompany.educationerpforgansu.commonpart.views.GDGridView
                android:id="@+id/meetingapply_rymd_grd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:numColumns="5"
                android:horizontalSpacing="@dimen/common_5"
                android:verticalSpacing="@dimen/common_5"
                ></com.htcompany.educationerpforgansu.commonpart.views.GDGridView>
            </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/common_5"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:text="主要内容"
            android:textColor="@color/black2"
            android:textSize="@dimen/textSize_14"
            />
        c<EditText
            android:id="@+id/meetingapply_content_edt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/common_10"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="@dimen/common_20"
            android:layout_marginRight="@dimen/common_20"
            android:hint="输入会议主要内容"
            android:textColorHint="@color/login_line_bg"
            android:background="@drawable/login_shape_editxt"
            android:lines="3"
            android:gravity="left|top"
            android:textColor="@color/title_bar_color"
            android:textSize="@dimen/textSize_14"
            android:layout_marginBottom="@dimen/common_10"
            />
    </LinearLayout>
    </LinearLayout></ScrollView>
</LinearLayout>