<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="true"
    android:fitsSystemWindows="true"
    android:background="@color/activity_bg_color"
    >
    <include
        layout="@layout/common_title_bar"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="选择教学班"
        android:textColor="@color/jwgl_tv4_color"
        android:textColorHint="@color/jwgl_tv4_color"
        android:gravity="center"
        android:drawableRight="@mipmap/jwgl_right"
        android:padding="@dimen/common_10"
        android:textSize="@dimen/textSize_14"
        android:background="@color/jwgl_search_bgcolor"
        />
    <com.htcompany.educationerpforgansu.commonpart.views.swipemenulistview.SwipeMenuListView
        android:id="@+id/studnetrewarsdmanage_lv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/activity_bg_color"
        android:dividerHeight="@dimen/common_3"
        android:cacheColorHint="#00000000"
        android:listSelector="#00000000"
        android:scrollbars="none"
        />
</LinearLayout>